<maui:MauiWinUIApplication
    x:Class="PopupPickers.WinUI.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:maui="using:Microsoft.Maui"
    xmlns:local="using:PopupPickers.WinUI">
    <maui:MauiWinUIApplication.Resources>
        <SolidColorBrush x:Key="ComboBoxItemForegroundPointerOver" Color="LightGreen" /> 
        <SolidColorBrush x:Key="ComboBoxDropDownBackground" Color="#444444" />
        <SolidColorBrush x:Key="ComboBoxItemForeground" Color="White" />
        <SolidColorBrush x:Key="ComboBoxForegroundPointerOver" Color="White" />
        <SolidColorBrush x:Key="ComboBoxItemForegroundSelected" Color="White" />
        <SolidColorBrush x:Key="ComboBoxItemForegroundSelectedPointerOver" Color="LightGreen" />
        <SolidColorBrush x:Key="ComboBoxItemForegroundPressed" Color="Yellow" />
        <SolidColorBrush x:Key="ComboBoxForegroundPressed" Color="White" />
        <SolidColorBrush x:Key="ComboBoxItemForegroundSelectedPressed" Color="LightGreen" />
        <SolidColorBrush x:Key="ComboBoxItemBackgroundSelected" Color="#777777" />
        <SolidColorBrush x:Key="ComboBoxItemBackgroundSelectedPointerOver" Color="#777777" />
        <SolidColorBrush x:Key="ComboBoxItemBackgroundSelectedPressed" Color="#777777" />
    </maui:MauiWinUIApplication.Resources>
</maui:MauiWinUIApplication>
